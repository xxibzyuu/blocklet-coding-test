{
  "name": "blocklet-coding-test",
  "version": "0.1.0",
  "scripts": {
    "dev": "blocklet dev",
    "start": "vite --host",
    "lint": "eslint src --ext .mjs,.js,.vue",
    "lint:fix": "npm run lint -- --fix",
    "serve": "vite preview",
    "clean": "node scripts/build-clean.js",
    "bundle": "npm run clean && npm run build && blocklet bundle --zip --create-release",
    "build": "vite build",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle --app-id zNKbiHZ88aazcH1vcQpsCR5Dkm1RCr6Ct39A",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "prepare": "husky install",
    "bump-version": "zx --quiet scripts/bump-version.mjs",
    "test": "jest"
  },
  "dependencies": {
    "ant-design-vue": "^1.7.2",
    "axios": "^1.6.0",
    "clipboard": "^2.0.11",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "vite": "^4.5.0",
    "vue": "^2.7.15",
    "vue-clipboard2": "^0.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/test-utils": "^1.1.3",
    "bumpp": "^8.2.1",
    "cypress": "^13.4.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-babel": "^1.0.1",
    "jsdom": "^22.1.0",
    "lint-staged": "^12.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "sass": "^1.69.5",
    "vite-plugin-blocklet": "^0.6.15",
    "vite-plugin-node-polyfills": "^0.7.0",
    "zx": "^7.2.3"
  },
  "lint-staged": {
    "*.{mjs,js,vue}": [
      "prettier --write",
      "eslint --ext .js,.vue,.mjs"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  }
}
